<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/fragmentContacts">

    <fragment
        android:id="@+id/fragmentContacts"
        android:name="com.example.phonebooktestapp.overui.FragmentContacts"
        android:label="" >
        <action
            android:id="@+id/action_fragmentContacts_to_detailFragment"
            app:destination="@id/detailFragment" />
    </fragment>
    <fragment
        android:id="@+id/detailFragment"
        android:name="com.example.phonebooktestapp.detailui.DetailFragment"
        android:label="DetailFragment" >
        <argument
            android:name="selectedContact"
            app:argType="com.example.phonebooktestapp.database.Contact"
            app:nullable="true"
            android:defaultValue="@null"/>
        <action
            android:id="@+id/action_detailFragment_to_fragmentContacts"
            app:destination="@id/fragmentContacts" />
    </fragment>
</navigation>